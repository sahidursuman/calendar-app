
var ready = function() {

	$('.calendar').fullCalendar({
		header: {
	    		left: 'title',
	    		center: 'prev next',
	    		right: 'today month,agendaWeek,agendaDay' 
	    	},

	     events: '/users/<%= escape_javascript(render params)  %>/availabilities.json',
	});
}

$(document).ready(ready);
$(document).on('page:load', ready);
