<%= form_for @instructor, remote: true  do |f| %>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label :first_language %>
    <%= f.string_field :first_language %>
  </div>  

  <div class="field">
    <%= f.label :subject_to_teach %>
    <%= f.string_field :subject_to_teach %>
  </div> 

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
:description, :first_language, :subject_to_teach, :education, :price_cents, :skype