<%= bootstrap_form_for(@instructor, layout: :horizontal, label_col: "col-sm-2", control_col: "col-sm-8" ) do |f| %>
	<%# f.text_field :country_of_origin %>
	<%= f.form_group :country, label: {text: "Country"} do %>
	  <%= f.country_select :country_of_origin, {priority_countries: ["US", "CA", "GB"], prompt: 'Please select a country'}, class: 'form-control' %>
	  <% end %>
	<%= f.text_field :first_language %>
	<%= f.text_field :subject_to_teach %>
	<%= f.text_field :education %>
	<%= f.number_field :price_cents %>
	<%= f.text_field :skype %>
	<%= f.text_area :description %>
	<%= f.form_group do  %>
	  <%= f.primary "Register" %> 
  <% end %>
<% end %>
